
term derp [n: #Nat]: #Nat ! #Console
 = box proc
   seq x = yield 5;
   seq y = yield the <foo: #Nat> of `foo 4;
   seq call #console'print "derp";
   when #nat'eq [x, 0] then return 3 done;
   cell n: #Nat ← 0;
   loop call #console'print "derp" done;
   yield [3]


