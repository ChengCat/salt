
term derp [n: #Nat]: #Nat ! #Console
 = box  proc #Nat of
   let  x = 5
   let  y = the <foo: #Nat> of `foo 4
   seq  #console'print "derp"
   seq  if #nat'eq [x, 0] then return 3
   seq  case y of { foo [x: #Nat] → return x }
   cel  n: #Nat ← 0
   seq  return 5
   end  with 0

