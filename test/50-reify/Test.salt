

term print @[a: #Data] [x: a]: [] !#Console
 = box #console'println (#reify'pretty @a x)

term fac [x: #Nat]: #Nat
 = launch #Nat of
   do   cell n: #Nat ← x
        cell a: #Nat ← 1
        loop do
                when (#nat'eq [n, 0]) break
                a ← #nat'mul [a, n]
                n ← #nat'sub [n, 1]
                end
        return a

emit #bundle'new
        [ [set #Symbol|]
        , [set #Symbol| 'fac] ]

